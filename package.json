{
  "name": "diff-js-xml",
  "version": "1.0.5",
  "description": "XML JSON object compare module",
  "repository": {
    "type": "git",
    "url": "git://github.com/jvdieten/diff-js-xml"
  },
  "main": "dist/src/index",
  "typings": "dist/src/index",
  "files": [
    "lib/**/*",
    "dist/src/index.js",
    "dist/src/index.d.ts"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:watch": "npm run test -- --watch --watch-extensions ts",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "clean": "rimraf dist",
    "lint": "tslint -t stylish --project  \"tsconfig.json\"",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run clean && tsc -p tsconfig.production.json",
    "build:watch": "tsc -w -p tsconfig.production.json",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "keywords": [
    "xml compare",
    "json compare",
    "xmldiff",
    "json diff",
    "xml2xml diff",
    "json2json diff"
  ],
  "author": "Joost van Dieten <joost.van.dieten@testassured.nl> (http://www.testassured.nl)",
  "license": "MIT",
  "dependencies": {
    "underscore": "^1.9.1",
    "xml-js": "^1.6.8"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.3",
    "@types/underscore": "^1.8.9",
    "@types/xml-js": "^1.0.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "onchange": "^5.1.0",
    "prettier": "^1.15.1",
    "sinon": "^4.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6",
    "rimraf": "~3.0.2"
  }
}
